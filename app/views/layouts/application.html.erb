<!DOCTYPE html>
<html>
<head>
  <title>Touch</title>
  <%= stylesheet_link_tag    "application" %>
  <%= render 'layouts/shim' %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
</head>
<body>
  <%= render 'layouts/header' %>
  <div class="container-fluid">
    <div class="row-fluid">
      <div class="span12">
        <%= render 'carousel' %>
      </div>
    </div>
    <div class="row-fluid">
      <div class="span12">
        <%= yield %>
      </div>
    </div>
    <%= render 'layouts/footer' %>
  </div>
</body>
</html>
