<h1>Products</h1>


<div class="span12">
  <ul class="media-grid">
    <% @products.each do |product| %>
      <li>
        <%# <h6><%= product.name %>
        <%= link_to image_tag(product.image.thumb.url, :class => "thumbnail"), product %>
      <li>
    <% end %>
  </ul>
</div>

<%= will_paginate @products %>

